{
    "manifest_version": 3,
    "name": "Video Sources",
    "version": "1.0",
    "description": "Display video sources of a website",
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    },
    "action": {
        "default_title": "",
        "default_icon": {
            "16": "/img/icon16.png",
            "32": "/img/icon32.png",
            "48": "/img/icon48.png",
            "64": "/img/icon64.png",
            "96": "/img/icon96.png",
            "128": "/img/icon128.png",
            "256": "/img/icon256.png",
            "512": "/img/icon512.png"
        }
    },
    "icons": {
        "16": "/img/icon16.png",
        "32": "/img/icon32.png",
        "48": "/img/icon48.png",
        "64": "/img/icon64.png",
        "96": "/img/icon96.png",
        "128": "/img/icon128.png",
        "256": "/img/icon256.png",
        "512": "/img/icon512.png"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ]
}